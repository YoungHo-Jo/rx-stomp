{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "member-ordering": [
      false
    ],
    "no-console": false,
    "no-empty": [
      true,
      "allow-empty-catch",
      "allow-empty-functions"
    ],
    "object-literal-sort-keys": false,
    "quotemark": [
      true,
      "single"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "never",
          "arrays": "never",
          "functions": "never",
          "typeLiterals": "ignore"
        },
        "esSpecCompliant": true
      }
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ]
  },
  "rulesDirectory": []
}